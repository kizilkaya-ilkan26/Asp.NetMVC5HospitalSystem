    <!-- Page Content-->
    <div class="container px-2 px-lg-2">
        <!-- Heading Row-->
       
        <!-- Call to Action-->
        <div style="margin-left:450px">
            <h1>Hakkımızda</h1>
        </div>
        <p>Hakkımızda Kısmı</p>




        <!-- Content Row-->
        <div class="row gx-4 gx-lg-5">
            <div style="justify-content:space-between; text-align: center;padding-bottom: 20px ;">
                <h2>⭐️Hızlı Erişim Menüsü⭐️</h2>
            </div>

            <div class="card" style="width: 18rem;">
                <img class="card-img-top" src="https://previews.123rf.com/images/imagecatalogue/imagecatalogue1603/imagecatalogue160300004/53480528-.jpg" alt="Card image cap">
                <div class="card-body">
                    <h5 class="card-title">Hasta Girişi</h5>
                    <p class="card-text">Hastaların hak ettiği hizmetleri yararlana bilmek için sisteme üye girişi yapması gereklidir. Giriş Paneli Gitmek İçin</p>
                    <a href="/HastaLogin/Index/" class="btn btn-primary">Tıkla</a>
                </div>
            </div>

            <div class="card" style="width: 18rem;">
                <img class="card-img-top" src="https://img.freepik.com/free-vector/doctor-icon-avatar-white_136162-58.jpg" alt="Card image cap">
                <div class="card-body">
                    <h5 class="card-title">Doktor Girişi</h5>
                    <p class="card-text">Doktorların hak ettiği hizmetleri yararlana bilmek için sisteme üye girişi yapması gereklidir. Giriş Paneli Gitmek İçin</p>
                    <a href="/login/index/" class="btn btn-primary">Tıkla</a>
                </div>
            </div>
            <div class="card" style="width: 18rem;">
                <img class="card-img-top" src="https://icons.iconarchive.com/icons/aha-soft/free-large-boss/512/Admin-icon.png" alt="Card image cap">
                <div class="card-body">
                    <h5 class="card-title">Admin Girişi</h5>
                    <p class="card-text">Adminin hak ettiği hizmetleri yararlana bilmek için sisteme üye girişi yapması gereklidir. Giriş Paneli Gitmek İçin</p>
                    <a href="/login/index/" class="btn btn-primary">Tıkla</a>
                </div>
            </div>
            <div class="card" style="width: 18rem;">
                <img class="card-img-top" src="https://media.istockphoto.com/vectors/flat-color-secretary-icon-vector-id868797066" alt="Card image cap">
                <div class="card-body">
                    <h5 class="card-title">Sekreter Girişi</h5>
                    <p class="card-text">Sekreterin hak ettiği hizmetleri yararlana bilmek için sisteme üye girişi yapması gereklidir. Giriş Paneli Gitmek İçin</p>
